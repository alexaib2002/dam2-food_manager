<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.uem.dam.employee_manager.controllers.WelcomeScene">
   <bottom>
      <Button defaultButton="true" mnemonicParsing="false" onAction="#onActionStartButton" text="Enter administration page" textAlignment="CENTER" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <Label fx:id="welcomeLabel" text="Welcome again, %s!" BorderPane.alignment="CENTER">
         <font>
            <Font size="36.0" />
         </font></Label>
   </top>
   <center>
      <ImageView fitWidth="250.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <image>
            <Image url="@../assets/database.png" />
         </image></ImageView>
   </center>
</BorderPane>
